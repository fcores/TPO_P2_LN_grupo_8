import java.util.*;

public class Queue<E> {
    private LinkedList<E> queue;

    public Queue() {
        queue = new LinkedList<>();
    }

    public void offer(E element) {
        queue.offer(element);
    }

    public E poll() {
        return queue.poll();
    }

    public E peek() {
        return queue.peek();
    }

    public boolean isEmpty() {
        return queue.isEmpty();
    }

    public int size() {
        return queue.size();
    }
}
